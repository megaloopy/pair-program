<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2 text-center">
<h1>Questions-index</h1>
<p>This is where the main question posts will be</p>
<br>
<%= link_to "New Question", new_question_path, class:"btn btn-primary" %>
<br>
<% @questions.each do |q| %>
  <h3>Title: <%= q.title %></h3>
  <p>Body: <%= q.body %></p>
  <%= link_to "Edit", edit_question_path(q) %>
<% end %>
    </div>
  </div>
</div>